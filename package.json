{
  "name": "happo-plugin-storybook",
  "version": "4.1.0",
  "description": "A happo.io plugin for projects using Storybook",
  "type": "module",
  "exports": {
    ".": "./dist/happo.cjs",
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./manager": "./dist/manager.js",
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.js",
      "require": "./dist/preview.cjs"
    },
    "./package.json": "./package.json",
    "./legacy": "./dist/legacy"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "lint": "eslint src/",
    "test": "jest",
    "happo": "happo",
    "prepublishOnly": "yarn build",
    "build-local-storybook": "storybook build -c .storybook -o .happo-out",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "tsup",
    "build:watch": "npm run build -- --watch",
    "dev": "run-p build:watch 'storybook --quiet'",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "npm run build && auto shipit",
    "eject-ts": "zx scripts/eject-typescript.js",
    "storybook": "storybook dev -p 9009",
    "build-storybook": "storybook build"
  },
  "jest": {
    "testMatch": [
      "**/*-test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enduire/happo-plugin-storybook.git"
  },
  "keywords": [
    "screenshot",
    "testing",
    "storybook",
    "happo",
    "visual",
    "regression",
    "testing",
    "storybook-addons"
  ],
  "author": "Henric Trotzig <henric.trotzig@happo.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enduire/happo-plugin-storybook/issues"
  },
  "homepage": "https://github.com/enduire/happo-plugin-storybook#readme",
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.0",
    "@storybook/addon-essentials": "8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/api": "^7.6.7",
    "@storybook/blocks": "^8.0.0",
    "@storybook/components": "^8.0.0",
    "@storybook/core-events": "^8.0.0",
    "@storybook/manager": "^8.0.0",
    "@storybook/manager-api": "^8.0.0",
    "@storybook/preview": "^8.0.0",
    "@storybook/preview-api": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@storybook/theming": "^8.0.0",
    "@storybook/types": "^8.0.0",
    "@types/archiver": "^6.0.2",
    "@types/babel__plugin-transform-runtime": "^7",
    "@types/node": "^18.15.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.2.1",
    "archiver": "^3.0.0",
    "auto": "^11.0.4",
    "boxen": "^7.1.1",
    "dedent": "^1.5.1",
    "eslint": "^8.6.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-react": "^7.28.0",
    "happo.io": "^9.0.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tether": "^2.0.0",
    "rimraf": "^5.0.5",
    "storybook": "^8.0.0",
    "styled-components": "^4.1.3",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.2",
    "zx": "^7.2.3"
  },
  "packageManager": "yarn@4.0.2",
  "storybook": {
    "displayName": "Happo",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "bundler": {
    "exportEntries": [
      "src/constants.ts",
      "src/legacy/addon.tsx",
      "src/legacy/decorator.tsx"
    ],
    "managerEntries": [
      "src/manager.ts"
    ],
    "previewEntries": [
      "src/register.ts",
      "src/preview.ts"
    ],
    "nodeEntries": [
      "src/preset.ts",
      "src/happo.ts"
    ]
  }
}
